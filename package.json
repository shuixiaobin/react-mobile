{
  "name": "dva-webpack-boilerplate",
  "version": "2.0.0",
  "description": "Boilerplate for Dvajs + React + Webpack project with JSX, ES6 and decorator compilation",
  "repository": {
    "url": "https://github.com/axel10/dva-webpack-boilerplate"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development LOCAL_ENV=local BUILD_ENV=development webpack-dev-server --progress --colors --config build/webpack.dev.js",
    "dev:test": "cross-env NODE_ENV=development LOCAL_ENV=local BUILD_ENV=development webpack-dev-server --progress --colors --config build/webpack.dev.js",
    "dev:pro": "cross-env NODE_ENV=production LOCAL_ENV=local BUILD_ENV=development webpack-dev-server --progress --colors --config build/webpack.dev.js",
    "build": "cross-env NODE_ENV=production BUILD_ENV=production webpack --progress --colors --config build/webpack.prod.js",
    "pro": "cross-env NODE_ENV=production BUILD_ENV=production webpack --progress --colors --config build/webpack.prod.js",
    "test": "cross-env NODE_ENV=development BUILD_ENV=production webpack --progress --colors --config build/webpack.prod.js",
    "cdn": "node ./build/cdn.dirname.js",
    "dist": "npm run build --report"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate"
  ],
  "license": "MIT",
  "dependencies": {
    "antd-mobile": "^2.3.1",
    "cssnano": "^4.1.10",
    "dva": "^2.4.1",
    "history": "^4.7.2",
    "intersection-observer": "^0.7.0",
    "prop-types": "^15.7.2",
    "rc-form": "^2.4.8",
    "react": "^16.8.4",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.4",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@types/history": "^4.7.2",
    "@types/react": "^16.8.4",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-plugin-import": "^1.12.0",
    "body-scroll-lock": "^2.6.3",
    "clean-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "dva-loading": "^3.0.6",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^2.6.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-aspect-ratio-mini": "^1.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-px-to-viewport": "^1.1.0",
    "postcss-write-svg": "^3.0.1",
    "querystringify": "^2.1.1",
    "style-loader": "^0.21.0",
    "ts-loader": "^5.3.0",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-config-standard": "^7.1.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "vconsole": "^3.3.0",
    "webpack": "^4.18.0",
    "webpack-bundle-analyzer": "^3.5.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.4"
  }
}
